{
  "name": "es2015",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "skeleton-css": "^2.0.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-kanmu": "^9.1.0",
    "eslint-plugin-sorting": "0.0.1",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "cp node_modules/skeleton-css/css/*.css dist",
    "build:html": "sed \"s/{{GOOGLE_MAP_API_KEY}}/${GOOGLE_MAP_API_KEY}/\" src/index.html > dist/index.html",
    "build:js": "browserify -t [ babelify --presets [ es2015 react ] ] src/app.js -o dist/bundle.js",
    "build": "npm run build:css && npm run build:html && npm run build:js",
    "watch": "watchify src/app.js -o dist/bundle.js -dv",
    "clean": "rm dist/*"
  },
